{
  "name": "cards-against-humanity",
  "version": "1.0.0",
  "description": "Python/Django version of the cards for/against humanity game.",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr": "ng run cards-against-humanity:serve-ssr:dev",
    "start:tunnel": "ngrok http 8000 -host-header=\"localhost:8000\"",
    "start:server": "npm run activate && python manage.py runserver",
    "start:dev": "concurrently 'npm run start' 'npm run start:server'",
    "build": "ng build --prod=true && ng run cards-against-humanity:server:production",
    "build:fragment": "node ./data/buildschema.js",
    "aa": "ts-node ./ssr/ssr.ts",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "activate": "source ./bin/activate",
    "migrate:create": "python manage.py makemigrations",
    "migrate:run": "python manage.py migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victor-enogwe/cards-against-humanity.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/victor-enogwe/cards-against-humanity/issues"
  },
  "homepage": "https://github.com/victor-enogwe/cards-against-humanity#readme",
  "private": true,
