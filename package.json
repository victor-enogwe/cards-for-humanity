{
  "name": "cards-against-humanity",
  "version": "1.0.0",
  "description": "A party game for horrible people.",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr": "ng run cards-against-humanity:serve-ssr:production",
    "start:server": "yarn activate && python manage.py runserver",
    "start:tunnel": "ngrok http 8000 -host-header=\"localhost:8000\"",
    "prerender": "ng run cards-against-humanity:prerender",
    "start:dev": "concurrently 'yarn start' 'yarn start:server' 'yarn build:gql'",
    "build": "ng build --subresource-integrity",
    "build:prod": "yarn build --configuration=production --output-hashing none",
    "build:shell": "ng run cards-against-humanity:app-shell:production",
    "build:ssr": "yarn build:prod && ng run cards-against-humanity:server:production",
    "build:gql": "graphql-codegen --config codegen.yml",
    "cron": "python manage.py runcrons",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix && yarn format",
    "format": "prettier --ignore-path .gitignore \"client/**/*.+(ts|js|tsx)\" --write",
    "e2e": "ng e2e",
    "activate": "source ./env/bin/activate",
    "migrate:create": "python manage.py makemigrations",
    "migrate:run": "python manage.py migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victor-enogwe/cards-against-humanity.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/victor-enogwe/cards-against-humanity/issues"
  },
  "homepage": "https://github.com/victor-enogwe/cards-against-humanity#readme",
  "private": true,
