{
  "name": "cards-against-humanity",
  "version": "1.0.0",
  "description": "Python/Django version of the cards for/against humanity game.",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr": "ng run cards-against-humanity:serve-ssr:production",
    "start:tunnel": "ngrok http 8000 -host-header=\"localhost:8000\"",
    "start:server": "npm run activate && python manage.py runserver",
    "start:dev": "concurrently 'npm run start' 'npm run start:server'",
    "build": "ng build --configuration production=true && ng run cards-against-humanity:server:production",
    "build:fragment": "node ./data/buildschema.js",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "activate": "source ./bin/activate",
    "migrate:create": "python manage.py makemigrations",
    "migrate:run": "python manage.py migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victor-enogwe/cards-against-humanity.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/victor-enogwe/cards-against-humanity/issues"
  },
  "homepage": "https://github.com/victor-enogwe/cards-against-humanity#readme",
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.10",
    "@angular/cdk": "~12.2.10",
    "@angular/common": "~12.2.10",
    "@angular/compiler": "~12.2.10",
    "@angular/core": "~12.2.10",
    "@angular/forms": "~12.2.10",
    "@angular/material": "^12.2.10",
    "@angular/platform-browser": "~12.2.10",
    "@angular/platform-browser-dynamic": "^12.2.10",
    "@angular/platform-server": "~12.2.10",
    "@angular/router": "~12.2.10",
    "@apollo/client": "3.4.16",
    "@ng-bootstrap/ng-bootstrap": "10.0.0",
    "@nguniversal/common": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "angularx-social-login": "4.0.1",
    "apollo-angular": "2.6.0",
    "bootstrap": "5.1.1",
    "graphql": "^14.5.8",
    "ngx-cookie-service": "^2.3.0",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.10",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "~12.2.10",
    "@angular/compiler-cli": "~12.2.10",
    "@angular/language-service": "~12.2.10",
    "@nguniversal/builders": "^9.0.0-next.7",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "ajv-formats": "2.1.1",
    "concurrently": "^5.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.26.0",
    "jasmine-core": "~3.6.0",
