{
  "name": "cards-for-humanity",
  "version": "1.0.0",
  "description": "A party game for horrible people.",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr": "ng run cards-for-humanity:serve-ssr:production",
    "start:server": "python backend/manage.py runserver",
    "start:tunnel": "ngrok http 8000 -host-header=\"localhost:8000\"",
    "prerender": "ng run cards-for-humanity:prerender",
    "start:dev": "concurrently 'yarn start' 'yarn start:server' 'yarn build:gql'",
    "build": "ng build --subresource-integrity",
    "build:prod": "yarn build --configuration=production --output-hashing none",
    "build:shell": "ng run cards-for-humanity:app-shell:production",
    "build:ssr": "yarn build:prod && ng run cards-for-humanity:server:production",
    "build:gql": "graphql-codegen --config codegen.yml",
    "cron": "python manage.py runcrons",
    "test": "ng test",
    "lint": "ng lint",
    "lint:backend": "python -m black ./backend",
    "lint:fix": "ng lint --fix && yarn format",
    "format": "prettier --ignore-path .gitignore \"client/**/*.+(ts|js|tsx)\" --write",
    "e2e": "ng e2e",
    "migrate:fixtures": "./plugins/fixtures.js -d",
    "migrate:clear:admin": "find . -path '*/backend/migrations/*.pyc'  -delete",
    "migrate:clear:cfh": "find . -path '*/backend/migrations/*.py' -not -name '__init__.py' -not -name '0002_cfh.py' -delete",
    "migrate:clear": "yarn migrate:clear:admin && yarn migrate:clear:cfh",
    "migrate:reinstall": "pip install --upgrade --force-reinstall  Django",
    "migrate:create": "python backend/manage.py makemigrations",
    "migrate:run": "python backend/manage.py migrate",
    "migrate:load": "python backend/manage.py loaddata *.json",
    "celery-beat": "celery -A api beat -l info --without-heartbeat --without-gossip --without-mingle",
    "celery-worker": "celery -A api worker -l INFO"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victor-enogwe/cards-for-humanity.git"
  },
