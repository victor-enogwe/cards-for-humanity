{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cards-against-humanity": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush"
        }
      },
      "root": "",
      "sourceRoot": "client",
      "prefix": "cah",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "static/browser",
            "index": "client/index.html",
            "main": "client/main.ts",
            "polyfills": "client/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "client/assets",
              "client/manifest.webmanifest"
            ],
            "styles": [
              "client/styles/styles.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "allowedCommonJsDependencies": [
              "subscriptions-transport-ws"
            ],
            "serviceWorker": true,
            "ngswConfigPath": "ngsw-config.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "client/environments/environment.ts",
                  "with": "client/environments/environment.prod.ts"
                }
              ],
              "baseHref": "/",
              "deployUrl": "/static/browser",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "development": {
              "baseHref": "/",
              "deployUrl": "/"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {},
          "configurations": {
            "production": {
              "browserTarget": "cards-against-humanity:build:production"
            },
            "development": {
              "browserTarget": "cards-against-humanity:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "serve-ssr": {
          "builder": "./ssr/builders:ssr-dev-server",
          "options": {
            "browserTarget": "cards-against-humanity:build",
            "serverTarget": "cards-against-humanity:server",
            "host": "localhost",
            "port": 8000,
            "serverStartCommand": "python manage.py runserver"
          },
          "configurations": {
            "production": {
              "browserTarget": "cards-against-humanity:build:production",
              "serverTarget": "cards-against-humanity:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "cards-against-humanity:build:production",
            "serverTarget": "cards-against-humanity:server:production",
            "guessRoutes": false,
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {}
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cards-against-humanity:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "client/test.ts",
            "polyfills": "client/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "client/assets",
              "client/manifest.webmanifest"
            ],
            "styles": [
              "client/styles/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "client/**/*.ts",
              "client/**/*.html"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cards-against-humanity:serve:production"
            },
            "development": {
              "devServerTarget": "cards-against-humanity:serve:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "static/server",
            "main": "./ssr/ssr.ts",
            "tsConfig": "tsconfig.server.json",
            "sourceMap": true,
            "optimization": false
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "client/environments/environment.ts",
                  "with": "client/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            },
            "development": {
              "outputHashing": "none",
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "app-shell": {
          "builder": "@angular-devkit/build-angular:app-shell",
          "options": {
            "browserTarget": "cards-against-humanity:build",
            "serverTarget": "cards-against-humanity:server-shell",
            "route": "shell"
          },
          "configurations": {
            "production": {
              "browserTarget": "cards-against-humanity:build:production",
              "serverTarget": "cards-against-humanity:server-shell:production"
            }
          }
        },
        "server-shell": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "static/server",
            "main": "client/main.server.ts",
            "tsConfig": "tsconfig.server-shell.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "client/environments/environment.ts",
                  "with": "client/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        }
      }
    }
  },
  "defaultProject": "cards-against-humanity",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
